<?xml version="1.0"?>

<!--
  - metadata-views.xml
  -
  - Named metadata sets for UI rendering
  - or other metadata disseminations
  -->

<dspace-metadata-views>

  <!-- site views -->

  <metadata-view>
    <name>mdview:site-dsl-full</name>
    <scope>site-mdv-full</scope>
    <rule>mdview:site-dsl-full</rule>
  </metadata-view>

  <!-- community views  -->

  <metadata-view>
    <name>mdview:community-dsl-full</name>
    <scope>community-mdv-full</scope>
    <rule>mdview:?,mdview:community-dsl-full</rule>
  </metadata-view>

  <!-- collection views  -->

  <metadata-view>
    <name>mdview:collection-dsl-full</name>
    <scope>collection-mdv-full</scope>
    <rule>mdview:?,mdview:collection-dsl-full</rule>
  </metadata-view>

  <!-- item views  -->

  <!-- brief view is for base item splash page  -->

  <metadata-view>
    <name>mdview:item-dc-brief</name>
    <scope>item-mdv-brief</scope>
    <!-- rule means that if the owning collection defines an item view, that supercedes this -->
    <rule>^mdview:?,mdview:item-dc-brief</rule>
  </metadata-view>

  <!-- short view is for search result and browse item lists -->

  <metadata-view>
    <name>mdview:item-dc-short</name>
    <scope>item-mdv-brief</scope>
    <rule>mdview:item-dc-short</rule>
  </metadata-view>

  <!-- dc-html view is for meta tags in item html header -->

  <metadata-view>
    <name>mdview:item-dc-html</name>
    <scope>item-mdv-html</scope>
    <rule>mdview:item-dc-html</rule>
  </metadata-view>

  <!-- dc-gsoc view is for Google Scholar meta tags in item html header -->

  <metadata-view>
    <name>mdview:item-dc-gsch</name>
    <scope>item-mdv-gsch</scope>
    <rule>mdview:item-dc-gsch</rule>
  </metadata-view>

  <!-- full view is for dump of metadata on html page -->

  <metadata-view>
    <name>mdview:item-dc-full</name>
    <scope>item-mdv-full</scope>
    <rule>^mdview:?,mdview:item-dc-full</rule>
  </metadata-view>

  <!-- bitstream brief view is for bitstream metadata on item page -->

  <metadata-view>
    <name>mdview:bitstream-dsl-brief</name>
    <scope>bitstream-mdv-brief</scope>
    <rule>mdview:bitstream-dsl-brief</rule>
  </metadata-view>

  <!-- bitstream full view is for bitstream metadata on bitstream page -->

  <metadata-view>
    <name>mdview:bitstream-dsl-full</name>
    <scope>bitstream-mdv-full</scope>
    <rule>mdview:bitstream-dsl-full</rule>
  </metadata-view>

